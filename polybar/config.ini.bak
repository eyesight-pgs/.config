
; polybar config


; bar
; https://github.com/polybar/polybar/wiki/Configuration#bar-settings
[bar/bar1]
; use default monitor
monitor =
; bar location top
bottom = true
fixed-center = false
width =
height =
offset-x = 0
offset-y = 0
; background color
background = #f00
; foreground color
foreground = #000
; background gradient: background[0-9]+ = #aarrggbb
background-0 = 
radius = 0.0
border-size = 
border-color = 
padding = 0
module-margin = 0
font-0 = 
; Modules are added to one of the available blocks
;   modules-left = cpu ram
;   modules-center = xwindow xbacklight
;   modules-right = ipc clock
modules-left = cpuuu
modules-center =
modules-right =

separator = " | "
spacing = 0
; opacity bw 0.0 - 1.0
dim-value = 1.0

; left | center | right | none
tray-position = right

dpi-x = 96
dpi-y = 96

; Fallback click handlers that will be called if
; there's no matching module handler found.
click-left = 
click-middle = 
click-right =
scroll-up =
scroll-down =
double-click-left =
double-click-middle =
double-click-right =
; in ms
double-click-interval = 400




[global/wm]
; Adjust the _NET_WM_STRUT_PARTIAL top value
;   Used for top aligned bars
; Supports any percentage with offset relative to the monitor height
margin-bottom = 500

; Adjust the _NET_WM_STRUT_PARTIAL bottom value
;   Used for bottom aligned bars
; Supports any percentage with offset relative to the monitor height
margin-top = 0




; Defines a module named 'mymodule'
[module/cpuuu]
; The type of this module
; The wiki pages for the individual modules show the correct values for each module.
type = 

; Hide the module by default
; Default: false
; New in version 3.6.0
hidden = false










