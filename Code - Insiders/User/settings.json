{
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "CI.ignoreSymbols": true,
  "cSpell.userWords": [
    "datatable",
    "ifnull",
    "joinee",
    "kolkata",
    "luxon",
    "Stallman"
  ],
  "debug.inlineValues": "on",
  "debug.toolBarLocation": "docked",
  "diffEditor.maxComputationTime": 0,
  "diffEditor.maxFileSize": 0,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.indentation": false,
  "editor.columnSelection": false,
  "editor.cursorWidth": 2,
  "editor.find.cursorMoveOnType": true,
  "editor.fontFamily": "'FiraCode Nerd Font Mono','Fira Code', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.insertSpaces": false,
  "editor.maxTokenizationLineLength": 200000,
  "editor.minimap.maxColumn": 150,
  "editor.minimap.renderCharacters": true,
  "editor.minimap.scale": 3,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "right",
  "editor.minimap.size": "fill",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "trailing",
  "editor.rulers": [
    80
  ],
  "editor.tabSize": 2,
  "editor.suggest.showStatusBar": true,
  "editor.tabCompletion": "on",
  "editor.trimAutoWhitespace": false,
  "explorer.autoReveal": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "Jenkinsfile": "groovy"
  },
  "files.eol": "\n",
  "files.hotExit": "off",
  "files.restoreUndoStack": false,
  "files.simpleDialog.enable": true,
  "files.trimTrailingWhitespace": false,
  "json.maxItemsComputed": 50000,
  "git.autofetch": true,
  "git.confirmSync": false,
  "intelephense.environment.phpVersion": "7.4.7",
  "intelephense.telemetry.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "php.validate.run": "onType",
  "rest-client.disableHighlightResonseBodyForLargeResponse": false,
  "rest-client.enableCustomVariableReferencesCodeLens": false,
  "rest-client.enableTelemetry": false,
  "sash.hoverBorder": "#f00",
  "search.searchEditor.defaultNumberOfContextLines": 0,
  "search.searchEditor.reusePriorSearchConfiguration": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "telemetry.telemetryLevel": "off",
  "terminal.external.windowsExec": "E:/apps/pwsh/pwsh.exe",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.useWslProfiles": true,
  "terminal.integrated.shell.linux": "bash",
  "terminal.integrated.shell.windows": "G:/apps/pwsh/pwsh.exe",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.useSystemClipboard": true,
  "window.dialogStyle": "custom",
  "window.menuBarVisibility": "compact",
  "window.restoreWindows": "none",
  "window.title": "⛈ ${rootName} ⛈",
  "window.titleBarStyle": "custom",
  "workbench.colorCustomizations": {
    "[Default Dark+]": {
      "editor.background": "#000000",
      "activityBar.activeBackground": "#111111",
      "activityBar.background": "#000000",
      "activityBar.inactiveForeground": "#333333",
      "activityBarBadge.background": "#333333",
      "activityBarBadge.foreground": "#bbbbbb",
      "editor.lineHighlightBackground": "#ffffff10",
      "editor.lineHighlightBorder": "#ffffff00",
      "editorGroup.border": "#777777",
      "editorGroup.emptyBackground": "#aaaaaa",
      "editorGroupHeader.tabsBackground": "#0e000e",
      // "editorGroupHeader.tabsBorder": "#222222",
      "editorGroupHeader.border": "#222222",
      // "editorGutter.background": "#000000",
      "editorLineNumber.foreground": "#666666",
      "editorPane.background": "#110011",
      "menu.background": "#070707",
      // "menubar.selectionBackground": "#ffffff",
      // "menubar.selectionForeground": "#000000",
      "minimap.background": "#131010",
      "minimap.errorHighlight": "#000000",
      "statusBar.background": "#111111",
      "statusBar.noFolderBackground": "#110011",
      "tab.activeBackground": "#151515",
      "tab.activeForeground": "#ffffff",
      "tab.hoverBackground": "#eeeeee",
      "tab.hoverForeground": "#000000",
      "tab.inactiveBackground": "#111111",
      "tab.inactiveForeground": "#eeeeee",
      "tab.border": "#000000",
      "tab.activeBorder": "#ffffff",
      "tab.activeBorderTop": "#ffffff",
      "titleBar.activeBackground": "#000000",
      "titleBar.inactiveBackground": "#110011",
      // "titleBar.inactiveForeground": "#ffffff",
    },
    "editorBracketHighlight.foreground1": "#ff9900",
    "editorBracketHighlight.foreground2": "#ffd900",
    "editorBracketHighlight.foreground3": "#09ff00",
    "editorBracketHighlight.foreground4": "#001aff",
    "editorBracketHighlight.foreground5": "#ff00b3",
    "editorBracketHighlight.foreground6": "#ffffff",
    "editorBracketPairGuide.activeBackground1": "#ff9900",
    "editorBracketPairGuide.activeBackground2": "#ffd900",
    "editorBracketPairGuide.activeBackground3": "#09ff00",
    "editorBracketPairGuide.activeBackground4": "#001aff",
    "editorBracketPairGuide.activeBackground5": "#ff00b3",
    "editorBracketPairGuide.activeBackground6": "#ffffff",
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.focusRecentEditorAfterClose": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.wrapTabs": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.sash.size": 10,
  "workbench.sideBar.location": "right",
  "workbench.editor.splitInGroupLayout": "vertical",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 20,
  "workbench.tree.renderIndentGuides": "always",
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "zenMode.hideTabs": false,
  "workbench.layoutControl.type": "toggles",
  "window.enableMenuBarMnemonics": false,
  "window.customMenuBarAltFocus": false,
  "debug.javascript.terminalOptions": {
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
  "Lua.telemetry.enable": false,
  "editor.minimap.enabled": false,
  "vim.leader": ";",
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "leader",
        "w"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "leader",
        "q"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "leader",
        "f","f"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "a"
      ],
      "after": [
        "<esc>"
      ]
    },
    {
      "before": [
        "leader",
        "w"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "a"
      ],
      "after": [
        "<esc>"
      ]
    }
  ],
  "diffEditor.ignoreTrimWhitespace": false
}