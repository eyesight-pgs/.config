{
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "cSpell.userWords": [
    "backpressure",
    "bcrypt",
    "bson",
    "classpath",
    "datatable",
    "ifnull",
    "joinee",
    "kolkata",
    "luxon",
    "middlewares",
    "mkdir",
    "neovim",
    "nvim",
    "respawn",
    "Stallman",
    "typeorm",
    "updation",
    "upgrader",
    "zipcode"
  ],
  "debug.inlineValues": "on",
  "debug.javascript.terminalOptions": {
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.maxComputationTime": 0,
  "diffEditor.maxFileSize": 0,
  "editor.bracketPairColorization.enabled": true,
  "editor.columnSelection": false,
  "editor.cursorWidth": 2,
  "editor.find.cursorMoveOnType": true,
  "editor.fontFamily": "'FiraCode Nerd Font Mono','Fira Code', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.guides.bracketPairs": "active",
  "editor.guides.indentation": false,
  "editor.insertSpaces": false,
  "editor.largeFileOptimizations": false,
  "editor.maxTokenizationLineLength": 200000,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 150,
  "editor.minimap.renderCharacters": true,
  "editor.minimap.scale": 3,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "right",
  "editor.minimap.size": "fill",
  "editor.multiCursorLimit": 100000,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": {
    "comments": "off",
    "other": "off",
    "strings": "off"
  },
  "editor.rename.enablePreview": false,
  "editor.renderWhitespace": "trailing",
  "editor.rulers": [
    80
  ],
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.suggest.showStatusBar": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Default Dark Modern]": {
      "textMateRules": [
        {
          "scope": [
            "keyword.control.flow.ts",
          ],
          "settings": {
            "fontStyle": "bold underline"
          }
        }
      ]
    }
  },
  "editor.trimAutoWhitespace": false,
  "explorer.autoReveal": false,
  "explorer.compactFolders": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "Jenkinsfile": "groovy"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "files.hotExit": "off",
  "files.restoreUndoStack": false,
  "files.simpleDialog.enable": true,
  "files.trimTrailingWhitespace": false,
  "git.openRepositoryInParentFolders": "always",
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "prompt",
  "json.maxItemsComputed": 50000,
  "php.validate.run": "onType",
  "rest-client.disableHighlightResonseBodyForLargeResponse": false,
  "rest-client.enableCustomVariableReferencesCodeLens": false,
  "rest-client.enableSendRequestCodeLens": true, // what is this?
  "rest-client.enableTelemetry": false,
  "search.searchEditor.defaultNumberOfContextLines": 0,
  "search.searchEditor.reusePriorSearchConfiguration": true,
  "cSpell.diagnosticLevel": "Hint",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.useWslProfiles": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.insertModeKeyBindings": [
    {
      "after": [
        "<esc>"
      ],
      "before": [
        "<leader>",
        "a"
      ]
    }
  ],
  "vim.leader": ";",
  "vim.normalModeKeyBindings": [
    {
      "after": [
        "<esc>"
      ],
      "before": [
        "<leader>",
        "a"
      ]
    },
    {
      "before": [
        "<leader>",
        "w"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "<leader>",
        "q"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    }
  ],
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindings": [
    {
      "after": [
        "<esc>"
      ],
      "before": [
        "<leader>",
        "a"
      ]
    }
  ],
  // "vscode-neovim.logLevel": "debug",
  // "vscode-neovim.neovimExecutablePaths.darwin": "",
  // "vscode-neovim.neovimExecutablePaths.linux": "/path/to/nvim",
  // "vscode-neovim.neovimExecutablePaths.win32": "",
  // "vscode-neovim.neovimInitVimPaths.linux": "/path/to/init.vim",
  // "vscode-neovim.useWSL": true,
  "window.commandCenter": false,
  "window.customMenuBarAltFocus": false,
  "window.density.editorTabHeight": "compact",
  "window.dialogStyle": "custom",
  "window.enableMenuBarMnemonics": false,
  "window.menuBarVisibility": "compact",
  "window.restoreWindows": "none",
  "window.title": "⛈ ${dirty} ${rootName} ⛈",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 1,
  "workbench.colorCustomizations": {
    "[Default Dark Modern]": {
      "activityBar.activeBackground": "#111111",
      "activityBar.background": "#000000",
      "activityBar.inactiveForeground": "#333333",
      "activityBarBadge.background": "#333333",
      "activityBarBadge.foreground": "#bbbbbb",
      "editor.background": "#000000",
      "editor.lineHighlightBackground": "#ffffff10",
      "editor.lineHighlightBorder": "#ffffff00",
      "editorGroup.border": "#777777",
      "editorGroup.emptyBackground": "#aaaaaa",
      "editorGroupHeader.border": "#222222",
      "editorGroupHeader.tabsBackground": "#0e000e",
      "editorLineNumber.foreground": "#666666",
      "editorPane.background": "#110011",
      "menu.background": "#070707",
      "minimap.background": "#131010",
      "minimap.errorHighlight": "#000000",
      "statusBar.background": "#111111",
      "statusBar.noFolderBackground": "#110011",
      "tab.activeBackground": "#eeeeee",
      "tab.activeBorder": "#ffffff",
      "tab.activeBorderTop": "#ffffff",
      "tab.activeForeground": "#000000",
      "tab.border": "#000000",
      "tab.hoverBackground": "#aa5050",
      "tab.inactiveBackground": "#111111",
      "tab.inactiveForeground": "#eeeeee",
      "titleBar.activeBackground": "#000000",
      "titleBar.inactiveBackground": "#110011"
    },
    "editorBracketHighlight.foreground1": "#ff9900",
    "editorBracketHighlight.foreground2": "#ffd900",
    "editorBracketHighlight.foreground3": "#09ff00",
    "editorBracketHighlight.foreground4": "#001aff",
    "editorBracketHighlight.foreground5": "#ff00b3",
    "editorBracketHighlight.foreground6": "#ffffff",
    "editorBracketPairGuide.activeBackground1": "#ff9900",
    "editorBracketPairGuide.activeBackground2": "#ffd900",
    "editorBracketPairGuide.activeBackground3": "#09ff00",
    "editorBracketPairGuide.activeBackground4": "#001aff",
    "editorBracketPairGuide.activeBackground5": "#ff00b3",
    "editorBracketPairGuide.activeBackground6": "#ffffff",
    "editorRuler.foreground": "#222",
    "terminal.findMatchBorder": "#ffff00"
  },
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.focusRecentEditorAfterClose": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.editor.splitInGroupLayout": "vertical",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.wrapTabs": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.layoutControl.type": "menu",
  "workbench.sash.hoverDelay": 0,
  "workbench.sash.size": 10,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.indent": 20,
  "workbench.tree.renderIndentGuides": "always",
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "zenMode.showTabs": "none"
}